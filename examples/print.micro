fn oop() {
    let self = struct {
        x: 1
        y: 2
        helper: fn(n) { return (x + y) * n }
    }
    return struct {
        f: fn [self] (n) {
            return self.helper(n) + self.x
        }
    }
}

println(oop().f(5))

