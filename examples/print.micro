fn fibonacci(n) {
    if (n > 1) {
        let result = fibonacci(n - 1) + fibonacci(n - 2)
        println(n, ": ", result)
        return result
    } else {
        return 1
    }
}

let result = fibonacci(5)

println("result: ", result)

